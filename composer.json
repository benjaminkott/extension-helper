{
    "name": "bk2k/extension-helper",
    "type": "composer-plugin",
    "description": "Helper for TYPO3 Extension Maintenance",
    "keywords": [
        "TYPO3 CMS",
        "Extension",
        "Development",
        "Maintenance"
    ],
    "homepage": "https://github.com/benjaminkott/extension-helper",
    "authors": [
        {
            "name": "Benjamin Kott",
            "email": "info@bk2k.info",
            "role": "Developer",
            "homepage": "http://www.bk2k.info/"
        }
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/benjaminkott/extension-helper/issues"
    },
    "require": {
        "symfony/console": "^3.4.17 || ^4.0",
        "composer-plugin-api": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "BK2K\\ExtensionHelper\\": "src/"
        }
    },
    "bin": [
        "extension-helper"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "class": "BK2K\\ExtensionHelper\\Composer\\Plugin"
    }
}
